      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#1A1735"
      },
      "package": "com.pfndesign.befit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
