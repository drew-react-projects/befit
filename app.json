{
  "expo": {
    "name": "befit",
    "slug": "befit",
    "description": "A Product By OneRed",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A1735"
    },
    "updates": {
